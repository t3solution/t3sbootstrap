										<numIndex index="1">2</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">both</numIndex>
										<numIndex index="1">3</numIndex>
									</numIndex>
								</items>
								<default>1</default>
							</config>
						</TCEforms>
					</bgimagePosition>
