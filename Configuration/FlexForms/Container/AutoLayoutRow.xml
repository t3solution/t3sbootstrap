										<numIndex index="0">xl</numIndex>
										<numIndex index="1">xl</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">xxl</numIndex>
										<numIndex index="1">xxl</numIndex>
									</numIndex>
								</items>
								<default></default>
								<size>1</size>
							</config>
						</TCEforms>
					</responsiveVariations>

					<horizontalGutters>
						<TCEforms>
							<label>Horizontal gutters</label>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">gx-0</numIndex>
										<numIndex index="1">gx-0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
