<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>General</sheetTitle>
				</TCEforms>
				<el>
					<hidden>
						<TCEforms>
							<label>Hiding / Display Elements</label>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">none</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">Hidden only on xs</numIndex>
										<numIndex index="1">d-none d-sm-block</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Hidden on sm and down</numIndex>
										<numIndex index="1">d-none d-md-block</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">Hidden on md and down</numIndex>
										<numIndex index="1">d-none d-lg-block</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">Hidden on lg and down</numIndex>
										<numIndex index="1">d-none d-xl-block</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">Hidden only on xl</numIndex>
										<numIndex index="1">d-xl-none</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">Visible only on xs</numIndex>
										<numIndex index="1">d-block d-sm-none</numIndex>
									</numIndex>
									<numIndex index="7" type="array">
										<numIndex index="0">Visible sm and down</numIndex>
										<numIndex index="1">d-block d-md-none</numIndex>
									</numIndex>
									<numIndex index="8" type="array">
										<numIndex index="0">Visible md and down</numIndex>
										<numIndex index="1">d-block d-lg-none</numIndex>
									</numIndex>
									<numIndex index="9" type="array">
										<numIndex index="0">Visible lg and down</numIndex>
										<numIndex index="1">d-block d-xl-none</numIndex>
									</numIndex>
								</items>
								<default>0</default>
							</config>
						</TCEforms>
					</hidden>
					<bgwlink>
						<TCEforms>
							<label>Link the entire Content Element (with header_link)</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</bgwlink>
				</el>
			</ROOT>
		</sDEF>

		<images>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>Image</sheetTitle>
				</TCEforms>
				<el>
					<origImage>
						<TCEforms>
							<label>Image inside (no background-image but a real image)</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</displayCond>
							<onChange>reload</onChange>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</origImage>
					<bgAttachmentFixed>
						<TCEforms>
							<label>Background-attachment - fixed</label>
							<displayCond>
								<AND>
									<numIndex index="1">FIELD:images.origImage:=:0</numIndex>
									<numIndex index="2">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</numIndex>
								</AND>
							</displayCond>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</bgAttachmentFixed>
					<enableAutoheight>
						<TCEforms>
							<label>Enable content overlay and autoheight for background-image (JS)</label>
							<onChange>reload</onChange>
							<displayCond>
								<AND>
									<numIndex index="1">FIELD:images.origImage:=:0</numIndex>
									<numIndex index="2">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</numIndex>
								</AND>
							</displayCond>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</enableAutoheight>
					<addHeight>
						<TCEforms>
							<label>Surcharge height to background-image (in px)</label>
							<displayCond>
								<AND>
									<numIndex index="0">FIELD:images.enableAutoheight:=:1</numIndex>
									<numIndex index="1">FIELD:images.origImage:=:0</numIndex>
									<numIndex index="2">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</numIndex>
									<OR>
										<numIndex index="0">FIELD:images.enableAutoheight:=:1</numIndex>
										<numIndex index="1">FIELD:images.origImage:=:1</numIndex>
										<numIndex index="2">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</numIndex>
									</OR>
								</AND>
							</displayCond>
							<config>
								<type>input</type>
								<checkbox></checkbox>
								<eval>int</eval>
								<size>6</size>
								<default>60</default>
							</config>
						</TCEforms>
					</addHeight>
					<alignItem>
						<TCEforms>
							<label>Align content items</label>
							<displayCond>
								<AND>
									<numIndex index="2">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</numIndex>
									<OR>
								  		<numIndex index="0">FIELD:images.enableAutoheight:=:1</numIndex>
								  		<numIndex index="1">FIELD:images.origImage:=:1</numIndex>
									</OR>
								</AND>
							</displayCond>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">none</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">start</numIndex>
										<numIndex index="1">align-self-start</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">end</numIndex>
										<numIndex index="1">align-self-end</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">center</numIndex>
										<numIndex index="1">align-self-center</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">baseline</numIndex>
										<numIndex index="1">align-self-baseline</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">stretch</numIndex>
										<numIndex index="1">align-self-stretch</numIndex>
									</numIndex>
								</items>
								<default>align-self-center</default>
							</config>
						</TCEforms>
					</alignItem>
					<paddingTopBottom>
						<TCEforms>
							<label>Padding (top and bottom in rem)</label>
							<displayCond>
								<AND>
									<numIndex index="0">FIELD:images.enableAutoheight:=:0</numIndex>
									<numIndex index="1">FIELD:images.origImage:=:0</numIndex>
									<numIndex index="2">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</numIndex>
									<OR>
										<numIndex index="0">FIELD:images.enableAutoheight:=:1</numIndex>
										<numIndex index="1">FIELD:images.origImage:=:0</numIndex>
										<numIndex index="2">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</numIndex>
									</OR>
								</AND>
							</displayCond>
							<config>
								<type>input</type>
								<checkbox></checkbox>
								<eval>int</eval>
								<size>6</size>
								<default>1</default>
							</config>
						</TCEforms>
					</paddingTopBottom>
					<imgGrayscale>
						<TCEforms>
							<label>Grayscale</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</displayCond>
							<config>
								<type>input</type>
								<size>5</size>
								<default>0</default>
								<eval>num</eval>
								<range>
									<lower>0</lower>
									<upper>100</upper>
								</range>
								<slider>
									<step>1</step>
									<width>200</width>
								</slider>
							</config>
						</TCEforms>
					</imgGrayscale>
					<imgSepia>
						<TCEforms>
							<label>Sepia</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</displayCond>
							<config>
								<type>input</type>
								<size>5</size>
								<default>0</default>
								<eval>num</eval>
								<range>
									<lower>0</lower>
									<upper>100</upper>
								</range>
								<slider>
									<step>1</step>
									<width>200</width>
								</slider>
							</config>
						</TCEforms>
					</imgSepia>
					<imgOpacity>
						<TCEforms>
							<label>Opacity</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</displayCond>
							<config>
								<type>input</type>
								<size>5</size>
								<default>100</default>
								<eval>num</eval>
								<range>
									<lower>0</lower>
									<upper>100</upper>
								</range>
								<slider>
									<step>1</step>
									<width>200</width>
								</slider>
							</config>
						</TCEforms>
					</imgOpacity>
					<imageRaster>
						<TCEforms>
							<label>Raster over the image/color</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isImage</displayCond>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</imageRaster>
				</el>
			</ROOT>
		</images>

		<background>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>No Media</sheetTitle>
				</TCEforms>
				<el>
					<noMediaPaddingTopBottom>
						<TCEforms>
							<label>Padding (top and bottom in rem)</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isNoMedia</displayCond>
							<config>
								<type>input</type>
								<checkbox></checkbox>
								<eval>int</eval>
								<size>6</size>
								<default>10</default>
							</config>
						</TCEforms>
					</noMediaPaddingTopBottom>
				</el>
			</ROOT>
		</background>

		<localvideo>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>Local Video</sheetTitle>
				</TCEforms>
				<el>
					<localControls>
						<TCEforms>
							<label>Controls</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isLocalVideo</displayCond>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</localControls>
					<loop>
						<TCEforms>
							<label>Loop</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isLocalVideo</displayCond>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</loop>
					<mute>
						<TCEforms>
							<label>Mute</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isLocalVideo</displayCond>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</mute>
					<aspectRatio>
						<TCEforms>
							<label>Aspect ratios</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isLocalVideo</displayCond>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">53by9</numIndex>
										<numIndex index="1">9/53</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">48by9</numIndex>
										<numIndex index="1">9/48</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">42by9</numIndex>
										<numIndex index="1">9/42</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">37by9</numIndex>
										<numIndex index="1">9/37</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">21by9</numIndex>
										<numIndex index="1">9/21</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">16by9</numIndex>
										<numIndex index="1">9/16</numIndex>
									</numIndex>
								</items>
								<default>9/37</default>
								<size>1</size>
							</config>
						</TCEforms>
					</aspectRatio>
					<alignVideoItem>
						<TCEforms>
							<label>Align content items</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isLocalVideo</displayCond>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">none</numIndex>
										<numIndex index="1">none</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">start</numIndex>
										<numIndex index="1">align-self-start</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">end</numIndex>
										<numIndex index="1">align-self-end</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">center</numIndex>
										<numIndex index="1">align-self-center</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">baseline</numIndex>
										<numIndex index="1">align-self-baseline</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">stretch</numIndex>
										<numIndex index="1">align-self-stretch</numIndex>
									</numIndex>
								</items>
								<default>align-self-center</default>
							</config>
						</TCEforms>
					</alignVideoItem>
					<shift>
						<TCEforms>
							<label>Extra vertical shift for the video</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isLocalVideo</displayCond>
							<description>e.g.: 100 or -100</description>
							<config>
								<type>input</type>
								<size>5</size>
								<default>0</default>
								<eval>int</eval>
								<range>
									<lower>-100</lower>
									<upper>100</upper>
								</range>
								<slider>
									<type>slider</type>
									<step>5</step>
									<width>200</width>
								</slider>
							</config>
						</TCEforms>
					</shift>
					<horizontalShift>
						<TCEforms>
							<label>Extra horizontal shift for the video on mobile (max-width:767px)</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isLocalVideo</displayCond>
							<description>e.g.: 100 or -100</description>
							<config>
								<type>input</type>
								<size>5</size>
								<default>0</default>
								<eval>int</eval>
								<range>
									<lower>-200</lower>
									<upper>200</upper>
								</range>
								<slider>
									<type>slider</type>
									<step>5</step>
									<width>200</width>
								</slider>
							</config>
						</TCEforms>
					</horizontalShift>
					<mobileHeight>
						 <TCEforms>
							 <label>Min. hight on mobile (max-width:767px)</label>
							 <description>card-img-overlay min-height in px</description>
							 <displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isLocalVideo</displayCond>
							 <config>
								<type>input</type>
								<size>5</size>
								<default>200</default>
								<eval>int</eval>
								<range>
									<lower>100</lower>
									<upper>1000</upper>
								</range>
								<slider>
									<type>slider</type>
									<step>5</step>
									<width>200</width>
								</slider>
							 </config>
						 </TCEforms>
					</mobileHeight>
					<mobileWidth>
						 <TCEforms>
							 <label>Mobile width for higher videos on mobile (max-width:767px)</label>
							 <description>figure.video width in %</description>
							 <displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isLocalVideo</displayCond>
							 <config>
								<type>input</type>
								<size>5</size>
								<default>100</default>
								<eval>int</eval>
								<range>
									<lower>100</lower>
									<upper>500</upper>
								</range>
								<slider>
									<type>slider</type>
									<step>5</step>
									<width>200</width>
								</slider>
							 </config>
						 </TCEforms>
					</mobileWidth>
				</el>
			</ROOT>
		</localvideo>

		<videoevents>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>Youtube/Vimeo Video</sheetTitle>
				</TCEforms>
				<el>
					<contentPosition>
						<TCEforms>
							<label>Content Position</label>
							<displayCond>
								<or>
									<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isYoutube</numIndex>
									<numIndex index="1">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isVimeo</numIndex>
								</or>
							</displayCond>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">none</numIndex>
										<numIndex index="1">none</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">start</numIndex>
										<numIndex index="1">align-self-start</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">end</numIndex>
										<numIndex index="1">align-self-end</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">center</numIndex>
										<numIndex index="1">align-self-center</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">baseline</numIndex>
										<numIndex index="1">align-self-baseline</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">stretch</numIndex>
										<numIndex index="1">align-self-stretch</numIndex>
									</numIndex>
								</items>
								<default>align-self-center</default>
							</config>
						</TCEforms>
					</contentPosition>
					<bgHeight>
						<TCEforms>
							<label>Height in px</label>
							<displayCond>
								<or>
									<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isYoutube</numIndex>
									<numIndex index="1">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isVimeo</numIndex>
								</or>
							</displayCond>
							<config>
								<type>input</type>
								<size>5</size>
								<default>300</default>
								<eval>int+</eval>
							</config>
						</TCEforms>
					</bgHeight>
					<ytshift>
						<TCEforms>
							<label>Extra shift for the video</label>
							<displayCond>
								<or>
									<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isYoutube</numIndex>
									<numIndex index="1">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isVimeo</numIndex>
								</or>
							</displayCond>
							<description>e.g.: -100 or 100 (default = 50)</description>
							<config>
								<type>input</type>
								<size>5</size>
								<default>50</default>
								<eval>int</eval>
								<range>
									<lower>-50</lower>
									<upper>150</upper>
								</range>
									<slider>
										<type>slider</type>
										<step>5</step>
										<width>200</width>
									</slider>
							</config>
						</TCEforms>
					</ytshift>
					<videoMute>
						<TCEforms>
							<label>Mute</label>
							<displayCond>
								<or>
									<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isYoutube</numIndex>
									<numIndex index="1">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isVimeo</numIndex>
								</or>
							</displayCond>
							<description>automatically activated if autoplay</description>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</videoMute>
					<videoControls>
						<TCEforms>
							<label>Controls</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isYoutube</displayCond>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</videoControls>
					<videoLoop>
						<TCEforms>
							<label>Loop</label>
							<displayCond>
								<or>
									<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isYoutube</numIndex>
									<numIndex index="1">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->isVimeo</numIndex>
								</or>
							</displayCond>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</videoLoop>
				</el>
