<T3DataStructure>
	<sheets>
		<general>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.settings</sheetTitle>
				</TCEforms>
				<el>
					<hidden>
						<TCEforms>
							<label>Hiding / Display Elements</label>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">none</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">Hidden only on xs</numIndex>
										<numIndex index="1">d-none d-sm-block</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Hidden on sm and down</numIndex>
										<numIndex index="1">d-none d-md-block</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">Hidden on md and down</numIndex>
										<numIndex index="1">d-none d-lg-block</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">Hidden on lg and down</numIndex>
										<numIndex index="1">d-none d-xl-block</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">Hidden only on xl</numIndex>
										<numIndex index="1">d-xl-none</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">Visible only on xs</numIndex>
										<numIndex index="1">d-block d-sm-none</numIndex>
									</numIndex>
									<numIndex index="7" type="array">
										<numIndex index="0">Visible sm and down</numIndex>
										<numIndex index="1">d-block d-md-none</numIndex>
									</numIndex>
									<numIndex index="8" type="array">
										<numIndex index="0">Visible md and down</numIndex>
										<numIndex index="1">d-block d-lg-none</numIndex>
									</numIndex>
									<numIndex index="9" type="array">
										<numIndex index="0">Visible lg and down</numIndex>
										<numIndex index="1">d-block d-xl-none</numIndex>
									</numIndex>
								</items>
								<default>0</default>
							</config>
						</TCEforms>
					</hidden>


					<display_type>
						<TCEforms>
							<onChange>reload</onChange>
							<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.appearance</label>
							<config type="array">
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">Tabs</numIndex>
										<numIndex index="1">tabs</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">Pills</numIndex>
										<numIndex index="1">pills</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Vertical Pills</numIndex>
										<numIndex index="1">verticalpills</numIndex>
									</numIndex>
								</items>
								<default>tabs</default>
							</config>
						</TCEforms>
					</display_type>
					<aside_width>
						<TCEforms>
							<label>Aside columns width</label>
							<displayCond>FIELD:display_type:=:verticalpills</displayCond>
							<config type="array">
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">2</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">3</numIndex>
										<numIndex index="1">3</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">4</numIndex>
										<numIndex index="1">4</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">6</numIndex>
										<numIndex index="1">6</numIndex>
									</numIndex>
								</items>
								<default>3</default>
							</config>
						</TCEforms>
					</aside_width>
					<switch_effect>
						<TCEforms>
							<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.effect</label>
							<config type="array">
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">none</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">fade</numIndex>
										<numIndex index="1">fade</numIndex>
									</numIndex>
								</items>
								<default>fade</default>
							</config>
						</TCEforms>
					</switch_effect>
					<fill>
						<TCEforms>
							<label>Fill and justify</label>
							<config type="array">
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">none</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">nav-fill</numIndex>
										<numIndex index="1">nav-fill</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">nav-justified</numIndex>
										<numIndex index="1">nav-justified</numIndex>
									</numIndex>
								</items>
								<default>0</default>
							</config>
						</TCEforms>
					</fill>
				</el>
			</ROOT>
		</general>
	</sheets>
</T3DataStructure>