								</numIndex>
								<numIndex index="7">
									<label>dark</label>
									<value>dark</value>
								</numIndex>
								<numIndex index="8">
									<label>white</label>
									<value>white</value>
								</numIndex>
								<numIndex index="9">
									<label>none</label>
									<value>0</value>
								</numIndex>
							</items>
							<default>primary</default>
						</config>
					</effectColor>

					<maxwidth>
