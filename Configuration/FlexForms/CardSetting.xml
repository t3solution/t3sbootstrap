									<value>180deg</value>
								</numIndex>
								<numIndex index="1">
									<label>540deg</label>
									<value>540deg</value>
								</numIndex>
								<numIndex index="2">
									<label>900deg</label>
									<value>900deg</value>
								</numIndex>
							</items>
							<default>180deg</default>
