<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Content</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<header.text>
						<TCEforms>
							<label>Card Header</label>
							<config>
								<type>input</type>
								<size>48</size>
								<max>99</max>
								<eval>trim</eval>
								<checkbox>0</checkbox>
							</config>
						</TCEforms>
					</header.text>
					<text.top>
						<TCEforms>
							<label>Text Top</label>
							<config>
								 <type>text</type>
								 <cols>int,trim</cols>
								 <rows>3</rows>
								 <enableRichtext>true</enableRichtext>
							</config>
						</TCEforms>
					</text.top>
					<list.container>
						<section>1</section>
						<type>array</type>
						<el>
							<list>
								<title>List-Item</title>
								<type>array</type>
								<el>
									<group>
										<TCEforms>
											<label>List Group</label>
											<config>
												<type>input</type>
												<size>48</size>
											</config>
										</TCEforms>
									</group>
								</el>
							</list>
						</el>
					</list.container>
					<text.bottom>
						<TCEforms>
							<label>Text Bottom (optional: new .card-block)</label>
							<config>
								 <type>text</type>
								 <cols>int,trim</cols>
								 <rows>3</rows>
								 <enableRichtext>true</enableRichtext>
							</config>
						</TCEforms>
					</text.bottom>
					<footer.text>
						<TCEforms>
							<label>Card Footer</label>
							<config>
								<type>input</type>
								<size>48</size>
								<max>99</max>
								<eval>trim</eval>
								<checkbox>0</checkbox>
							</config>
						</TCEforms>
					</footer.text>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>