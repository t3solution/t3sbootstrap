								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<wizards>
									<suggest>
										<type>suggest</type>
										<default>
											<searchWholePhrase>1</searchWholePhrase>
										</default>
									</suggest>
								</wizards>
							</config>
